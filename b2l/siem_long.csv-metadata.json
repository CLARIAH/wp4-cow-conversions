 "dc:title": "siem_long.csv",
 "@id": "https://iisg.amsterdam/resource/siem_long.csv",
 "dc:modified": {
  "@value": "2017-11-22",
  "@type": "xsd:date"
 },
 "tableSchema": {
  "aboutUrl": "https://iisg.amsterdam/siem/{_row}",
  "primaryKey": "city",
  "columns": [
   {
    "datatype": "string",
    "titles": [
     "city"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/city",
    "name": "city",
    "propertyUrl": "siem:vocab/city",
    "dc:description": "Name of the town or city in modern English spelling, with alternative spellings in brackets",
    "valueUrl": "siem:code/city/{city}"
   },
   {
    "datatype": "integer",
    "titles": [
     "uncountrycode"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/uncountrycode",
    "name": "uncountrycode",
    "propertyUrl": "siem:vocab/uncountrycode",
    "dc:description": "ISO-Assigned Numeric Code (3166-1)",
    "valueUrl": "siem:code/uncountrycode/{uncountrycode}"
   },
   {
    "virtual": true,
    "datatype": "int",
    "aboutUrl": "siem:code/uncountrycode/{uncountrycode}",
    "name": "uncountrycode",
        "titles": [
     "uncountrycode"
    ],
    "propertyUrl": "https://iisg.amsterdam/vocab/isoNumber",
    "csvw:value": "{uncountrycode}"
    },
   {
    "datatype": "string",
    "titles": [
     "country"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/country",
    "name": "country",
    "propertyUrl": "siem:vocab/country",
    "valueUrl": "siem:code/country/{% if country == 'UK' %}United Kingdom{% else %}{country}{% endif %}",
    "dc:description": "Name of the country in the year 2000"
   },
   {
   "virtual": true,
   "aboutUrl": "siem:code/country/{country}",
   "propertyUrl": "skos:exactMatch",
   "valueUrl": "sdr:resource/country/{country}"
   },
   {
    "datatype": "string",
    "titles": [
     "latitude"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/latitude",
    "name": "latitude",
    "propertyUrl": "siem:vocab/latitude",
    "dc:description": "Latitude"
   },
   {
    "datatype": "string",
        "titles": [
     "longitude"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/longitude",
    "name": "longitude",
    "propertyUrl": "siem:vocab/longitude",
    "dc:description": "Longitude"
   },
   {
    "virtual": true,
    "aboutUrl": "siem:code/city/{city}",
    "propertyUrl": "geosparql:hasGeometry",
    "valueUrl": "siem:code/genid/{{city}}"
   },
   {
    "virtual": true,
    "datatype": "geosparql:wktLiteral",
    "aboutUrl": "siem:code/genid/{{city}}",
    "propertyUrl": "geosparql:asWKT",
    "csvw:value": "{{'POINT(' + longitude + ' ' + latitude + ')'}}"
   },
   {
    "virtual": true,
    "aboutUrl": "siem:code/genid/{{city}}",
    "propertyUrl": "rdf:type",
    "valueUrl": "geosparql:Geometry"
   },
   {
    "datatype": "integer",
    "titles": [
     "elevation"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/elevation",
    "name": "elevation",
    "propertyUrl": "siem:vocab/elevation",
    "dc:description": "Elevation above sea level in meters"
   },
   {
    "datatype": "string",
    "titles": [
     "transportloc"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/transportloc",
    "name": "transportloc",
    "propertyUrl": "siem:vocab/transportloc",
    "valueUrl": "siem:code/transportloc/{transportloc}",
    "dc:description": "First nature geographical characteristics of the city: its fluvial catchment area and whether it was a ‘sea’, ‘river’ or ‘landlocked’ city"
   },
   {
    "datatype": "xsd:gYear",
    "titles": [
     "year"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/year",
    "name": "year",
    "propertyUrl": "siem:vocab/year",
    "csvw:value": "{year:0>4}",
    "dc:description": "Time window of observation (whole centuries from 700 to 1500, and half centuries from 1500 to 2000)"
   },
   {
    "datatype": "integer",
    "titles": [
     "inhab"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/inhab",
    "name": "inhab",
    "propertyUrl": "siem:vocab/inhab",
    "dc:description": "Numbers of inhabitants (in thousands) in the city at the time window in question"
   },
   {
    "datatype": "string",
    "titles": [
     "comment"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/comment",
    "name": "comment",
    "propertyUrl": "siem:vocab/comment",
    "dc:description": "Bibliographical references and other notes"
   },
   {
    "datatype": "string",
    "titles": [
     "quality"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/quality",
    "name": "quality",
    "propertyUrl": "siem:vocab/quality",
    "csvw:parseOnEmpty":true,
    "valueUrl": "siem:code/quality/{% if quality == 'rood' %}proxy{% elif quality == 'cursief' %}imputed{% else %}direct{% endif %}",
    "dc:description": "Quality of observation, either proxy, imputed, or directly derived from source ('direct')"
   },
   {
    "datatype": "string",
    "titles": [
     "city_alt"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/city_alt",
    "name": "city_alt",
    "propertyUrl": "siem:vocab/city_alt",
    "dc:description": "Alternative spellings of city or town name"
   },
   {
    "datatype": "string",
    "titles": [
     "city_short"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/city_short",
    "name": "city_short",
    "propertyUrl": "siem:vocab/city_short",
    "dc:description": "Name of the town or city in modern English spelling"
   },
   {
    "titles": [
     "city_safe"
    ],
    "@id": "https://iisg.amsterdam/resource/siem_long.csv/column/city_safe",
    "name": "city_safe",
    "propertyUrl": "siem:vocab/cityid",
    "dc:description": "city identifier created from short city name and rounded latitude and longitude",
    "valueUrl": "siem:code/cityid/{{city + latitude|int|string + '_' + longitude|int|string}}"
   }
  ]
 }
}
